<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="pers.flights.mapper.DatetestMapper" >

  
  <select id="test" resultType="pers.flights.model.Datetest" >
    select * from datetest where
    <if test="startTime != null">date &gt;= #{startTime}</if>
    <if test="startTime != null and endTime != null"> and </if>
    <if test="endTime != null">date &lt;= #{endTime}</if> 
  </select>
  
</mapper>